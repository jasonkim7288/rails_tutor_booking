<div class="row m-0">
  <div class="col-12 col-md-6 p-0">
    <%= image_tag 'https://images.unsplash.com/photo-1509062522246-3755977927d7?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1404&q=80', class: "main-img-1" %>
  </div>

  <div class="d-none d-md-block col-md-6 p-0">
    <%= image_tag 'https://images.unsplash.com/photo-1588702547919-26089e690ecc?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1350&q=80', class: "main-img-1" %>
  </div>

</div>


<h1>Tutor Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Place</th>
      <th>Category</th>
      <th>Start datetime</th>
      <th>End datetime</th>
      <th>Conf url</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Decimal</th>
      <th>Max students num</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutor_sessions.each do |tutor_session| %>
      <tr>
        <td><%= tutor_session.title %></td>
        <td><%= tutor_session.description %></td>
        <td><%= tutor_session.place %></td>
        <td><%= tutor_session.category %></td>
        <td><%= tutor_session.start_datetime %></td>
        <td><%= tutor_session.end_datetime %></td>
        <td><%= tutor_session.conf_url %></td>
        <td><%= tutor_session.address %></td>
        <td><%= tutor_session.latitude %></td>
        <td><%= tutor_session.longitude %></td>
        <td><%= tutor_session.decimal %></td>
        <td><%= tutor_session.max_students_num %></td>
        <td><%= tutor_session.user_id %></td>
        <td><%= link_to 'Show', tutor_session %></td>
        <td><%= link_to 'Edit', edit_tutor_session_path(tutor_session) %></td>
        <td><%= link_to 'Destroy', tutor_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tutor Session', new_tutor_session_path %>
